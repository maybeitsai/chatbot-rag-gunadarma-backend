# Google API Configuration
GOOGLE_API_KEY=your_google_api_key_here

# Model Configuration
LLM_MODEL=gemini-2.5-flash-preview-05-20
EMBEDDING_MODEL=models/text-embedding-004

# Database Configuration (Neon PostgreSQL with pgvector)
NEON_CONNECTION_STRING=postgresql://username:password@host:5432/database?sslmode=require

# Text Processing Configuration
CHUNK_SIZE=500
CHUNK_OVERLAP=50
MAX_TOKENS_PER_CHUNK=512

# Vector Store Configuration
VECTOR_STORE_COLLECTION=chatbot_gunadarma
HNSW_M=16
HNSW_EF_CONSTRUCTION=64
HNSW_EF_SEARCH=40

# Semantic Cache Configuration
CACHE_ENABLED=true
CACHE_SIMILARITY_THRESHOLD=0.85
CACHE_MAX_SIZE=1000
CACHE_TTL_HOURS=168

# Performance Configuration
MAX_WORKERS=4
BATCH_SIZE=50
SIMILARITY_SCORE_THRESHOLD=0.25
RETRIEVAL_K=8

# API Configuration
FASTAPI_HOST=0.0.0.0
FASTAPI_PORT=8000
FASTAPI_RELOAD=true
CORS_ORIGINS=*

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=app.log
OPTIMIZATION_LOG=optimization.log

# Data Processing Configuration
DATA_CLEANING_SIMILARITY_THRESHOLD=0.85
FORCE_REPROCESS=false
SAVE_INTERMEDIATE_FILES=true

# Optional: Redis Configuration (for distributed caching)
# REDIS_URL=redis://localhost:6379/0
# REDIS_ENABLED=false

# Development/Production Mode
ENVIRONMENT=development
DEBUG=true

# Security (for production)
# SECRET_KEY=your_secret_key_here
# ACCESS_TOKEN_EXPIRE_MINUTES=30

